{
  "name": "@jeff-aporta/camaleon",
  "version": "0.0.17",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jeff-Aporta/camaleon-ui"
  },
  "dependencies": {
    "color": "^5.0.0",
    "react-router-dom": "^7.4.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "serve": "serve -s build",
    "eject": "react-scripts eject",
    "transpile": "npx babel src/framework --out-dir dist",
    "replace-jsx": "node scripts/replace-jsx.js dist",
    "copy-scss": "node scripts/copy-scss.js",
    "build-package": "npm run transpile && npm run replace-jsx && npm run copy-scss",
    "version-bump": "npm version patch --no-git-tag-version --no-git-commit-hooks",
    "git-add-commit-push": "git add . && git commit -m \"ActualizaciÃ³n\" && git push origin main",
    "publish-package": "npm run build-package && npm publish --access public && npm run version-bump && npm run git-add-commit-push",
    "deploy-gh-pages": "cross-env-shell PUBLIC_URL=. react-app-rewired build && gh-pages -d build",
    "deploy-branch-repo": "cross-env-shell PUBLIC_URL=/ npm run build && gh-pages -d build -b %npm_config_branch%",
    "deploy-prod": "npm run deploy-branch-repo --branch=build-prod",
    "deploys": "npm run publish-package && npm run deploy-gh-pages && npm run deploy-prod"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "@mui/x-charts": "^7.28.0",
    "@mui/x-data-grid": "^7.28.1",
    "@mui/x-date-pickers": "^7.28.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@use-gesture/react": "^10.3.1",
    "date-fns": "^4.1.0",
    "react": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-dom": "^19.0.0",
    "cross-env": "^7.0.3",
    "cross-env-shell": "^1.0.0",
    "customize-cra": "^1.0.0",
    "gh-pages": "^6.3.0",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "5.0.1",
    "sass": "^1.86.0",
    "sharp": "^0.34.2",
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.10",
    "@babel/preset-react": "^7.26.3",
    "@babel/plugin-proposal-class-properties": "^7.26.0",
    "@babel/plugin-proposal-private-methods": "^7.26.0"
  }
}
